<%= form_with(model: @kitten) do |form|%>
    <%= form.label 'Name' %>
    <%= form.text_field :name %>
    <br>
    <%=form.label "Age"%>
    <%=form.text_field :age%>
    <br>
    <%=form.label 'Softness'%>
    <%=form.text_field :softness%>
    <br>
    <%=form.label "Cuteness"%>
    <%=form.text_field :cuteness%>
    <br>
    <%= form.submit "submit"%>
<%end%>